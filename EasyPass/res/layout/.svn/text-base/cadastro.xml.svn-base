<?xml version="1.0" encoding="utf-8"?>
<TableLayout
	android:id="@+id/widget33"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical"
	xmlns:android="http://schemas.android.com/apk/res/android">
<TextView
	android:id="@+id/Titulo_c"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:padding="5dp"
	android:text="EasyPass - Cadastro"
	android:textSize="25sp"
	android:textColor="#f00"
	android:layout_gravity="center_horizontal" />
<EditText
	android:id="@+id/txt_login_c"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:padding="5dp"
	android:hint="Login"
	android:textSize="18sp" />
<EditText
	android:id="@+id/txt_pass_c"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:padding="5dp"
	android:hint="Pass"
	android:textSize="18sp" />
<EditText
	android:id="@+id/txt_pass2_c"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:padding="5dp"
	android:hint="Pass"
	android:textSize="18sp" />
<EditText
	android:id="@+id/txt_perg_c"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:padding="5dp"
	android:hint="Pergunta Secreta"
	android:textSize="18sp" />
<EditText
	android:id="@+id/txt_resp_c"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:padding="5dp"
	android:hint="Resposta"
	android:textSize="18sp" />
<Button
	android:id="@+id/btn_cadastro_c"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:padding="5dp"
	android:text="Cadastro"
	android:textSize="18sp"
	android:textColor="#000" />
</TableLayout>
